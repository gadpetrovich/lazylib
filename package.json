{
  "name": "ts-lazy-lib",
  "author": "Шарапов Павел, subber@yandex.ru",
  "description": "Реализация ленивых вычислений в typescript",
  "version": "0.1.2",
  "main": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src __tests__",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "lazy",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/gadpetrovich/lazylib/issues"
  },
  "homepage": "https://github.com/gadpetrovich/lazylib#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gadpetrovich/lazylib.git"
  },
  "license": "MIT",
  "dependencies": {
    "@types/browserify": "latest",
    "browserify": "latest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.57.0",
    "eslint-plugin-eslint-plugin": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
